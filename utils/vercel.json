{
    "builds":[
        {
            "src": "main.py",
            "use": "@vercel/python"
        }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "destination": "main.py"
        }
    ],
    "rewrites": [
        { 
            "source": "/(.*)", 
            "destination": "/main.py" 
        }
    ]
    
}